{
  "buildCommand": "npm run docs:build",
  "outputDirectory": "site/.vitepress/dist",
  "installCommand": "npm install",
  "devCommand": "vitepress dev site --port $PORT",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/go/apps",
      "destination": "/kb/troubleshooting#app-compatibility",
      "permanent": true
    }
  ]
}
